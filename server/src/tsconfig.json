{
    "compilerOptions": {
        "target": "es6",
        "outDir": "..",
        "moduleResolution": "node"
    },
    "formatCodeOptions": {
        "indentSize": 2,
        "tabSize": 2
    },
    "filesGlob": [
        "../api.d.ts",
        "./**/*.ts",
        "../node_modules/*/model.d.ts",
        "../node_modules/marklogic-typescript-definitions/ts/index.d.ts"
    ],
    "compileOnSave": false,
    "files": [
        "../api.d.ts",
        "./service.ts",
        "./utils.ts",
        "../node_modules/markscript-core/model.d.ts",
        "../node_modules/markscript-uservices/model.d.ts",
        "../node_modules/marklogic-typescript-definitions/ts/index.d.ts"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
